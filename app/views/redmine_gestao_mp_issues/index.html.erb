<%= render partial: 'assets/import' %>

<% content_for :sidebar do %>
    <%= render :partial => 'redmine_gestao_mp_sidebar/content' %>
<% end %>

<h2><%=l(:redmine_gestao_mp_label_issues)%></h2>

<table id="redmine-gestao-mp-issues-table" data-issues="<%=@project_hierarchy%>" data-project-id="<%= @project.identifier %>">
	<colgroup>
		<col width="3%"></col>
		<col width="57%"></col>
		<col width="10%"></col>
		<col width="10%"></col>
		<col width="20%"></col>
	</colgroup>

	<thead>
		<tr> 
			<th></th> 
			<th></th> 
			<th><%= t(:redmine_gestao_mp_start_date) %></th>
			<th><%= t(:redmine_gestao_mp_end_date) %></th>
			<th><%= t(:redmine_gestao_mp_assigned_to) %></th>
		</tr>
	</thead>
	<!-- Optionally define a row that serves as template, when new nodes are created: -->
	<tbody>
		<tr>
			<td style="text-align: center;"></td>
			<td></td>
			<td></td>
			<td></td>
			<td></td>
		</tr>
	</tbody>
</table>

